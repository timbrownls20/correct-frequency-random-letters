    "@babel/preset-typescript": "^7.16.5",
    "@testing-library/jest-dom": "^5.16.1",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/webpack-env": "^1.16.3",
    "babel-jest": "^27.4.5",
    "babel-loader": "^8.2.3",
    "babel-preset-react": "^6.24.1",
    "jest": "^27.4.5",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.1",
