  "author": "Tim Brown",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/timbrownls20/correct-frequency-random-letters"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
